{
  "name": "prompt-monsters",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run format && npm run move-abi",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '**/*.js' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.yml' '**/*.sol' --ignore-path .gitignore && npm run lint",
    "typechain": "typechain --target ethers-v6 artifacts/*.json",
    "premove-abi": "cd contracts/prompt-monsters && npm run compile",
    "move-abi-prompt-monsters": "rm -rf artifacts/PromptMonsters.json && cd contracts/prompt-monsters && cp -rf artifacts/contracts/prompt-monsters/PromptMonsters.sol/PromptMonsters.json ../../artifacts/PromptMonsters.json",
    "move-abi": "npm run move-abi-prompt-monsters",
    "postmove-abi": "npm run typechain"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "clsx": "^1.2.1",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "ethers": "^6.3.0",
    "next": "13.3.0",
    "openai": "^3.2.1",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "18.2.0",
    "react-uuid": "^2.0.0",
    "recoil": "^0.7.7",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typechain/ethers-v6": "^0.3.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "next-intercept-stdout": "^1.0.1",
    "prettier": "^2.8.7",
    "prettier-plugin-solidity": "^1.1.3",
    "typechain": "^8.1.1"
  }
}
